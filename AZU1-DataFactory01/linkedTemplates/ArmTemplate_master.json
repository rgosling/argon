{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "AZU1-DataFactory01"
		},
		"ls_dev_sq01_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_dev_sq01'"
		},
		"ls_adls2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://azu1storage2.dfs.core.windows.net/"
		},
		"ls_dev_sq01_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "SQL01-Dev.aitx.com\\fms"
		},
		"ls_dev_sq01_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "SalesForceProdAll"
		},
		"ls_dev_sq01_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "ADFSQLUSER"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "AZU1-DataFactory01_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_dev_sq01_password": {
						"value": "[parameters('ls_dev_sq01_password')]"
					},
					"ls_adls2_properties_typeProperties_url": {
						"value": "[parameters('ls_adls2_properties_typeProperties_url')]"
					},
					"ls_dev_sq01_properties_typeProperties_server": {
						"value": "[parameters('ls_dev_sq01_properties_typeProperties_server')]"
					},
					"ls_dev_sq01_properties_typeProperties_database": {
						"value": "[parameters('ls_dev_sq01_properties_typeProperties_database')]"
					},
					"ls_dev_sq01_properties_typeProperties_userName": {
						"value": "[parameters('ls_dev_sq01_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}